[application]

config/name="TRPG"
run/main_scene="res://Scenes/Combat/CombatBase.tscn"
config/icon="res://icon.png"

[autoload]

EVENTS="*res://Global/Autoloads/Events.gd"
EXPLODE="*res://Global/Autoloads/Explode.gd"
GAME="*res://Global/Autoloads/GAME.gd"

[display]

window/size/width=640
window/size/height=360
window/size/fullscreen=true
window/size/test_width=640
window/size/test_height=360
window/vsync/use_vsync=false
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "Plugin" )

[importer_defaults]

texture={
